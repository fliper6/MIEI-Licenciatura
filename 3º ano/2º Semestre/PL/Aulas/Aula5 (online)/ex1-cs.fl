%{
#include "y.tab.h"
void yyerror(char* s);
%}
%option noyywrap  yylineno
%%
[ \n]       {}
#.*         {}

[,]         { return(yytext[0]); }
(?i:inicio) { return(INICIO); }
(?i:fim)    { return(FIM); }
[a-zA-Z]+   { return(pal); }
[0-9]+      { return(num); }

.           { yyerror("Simbolo desconhecido");     }
%%

