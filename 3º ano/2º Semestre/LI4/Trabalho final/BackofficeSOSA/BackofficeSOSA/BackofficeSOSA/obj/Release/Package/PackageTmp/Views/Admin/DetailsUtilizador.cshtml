@model BackofficeSOSA.Models.Utilizador

@{
    ViewBag.Title = "DetailsUtilizador";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}




<div class="form-horizontal" style="font-size:22px">
    <div class="col-md-offset-2 col-md-10">
        <h2>Detalhes Utilizador</h2>
    </div>
    <hr />

    <dl class="dl-horizontal col-md-offset-2 col-md-10">
        <dt>
            @Html.DisplayNameFor(model => model.Nome):
        </dt>

        <dd >
            @Html.DisplayFor(model => model.Nome)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Email):
        </dt>

        <dd >
            @Html.DisplayFor(model => model.Email)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Password):
        </dt>

        <dd >
            @Html.DisplayFor(model => model.Password)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.NrTelemovel):
        </dt>

        <dd >
            @Html.DisplayFor(model => model.NrTelemovel)
        </dd>

    </dl>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <a href="/Admin/EditUtilizador/@Model.Id" class="btn btn-info">Editar utilizador</a>
            <a onclick="window.history.back()" class="btn btn-warning text-white">Voltar</a>
        </div>
    </div>
</div>
